    "post": {
      "type": "application",
      "root": "apps/post",
      "entryFile": "main",
      "sourceRoot": "apps/post/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post/tsconfig.app.json"
      }
    },
    "file-upload": {
      "type": "application",
      "root": "apps/file-upload",
      "entryFile": "main",
      "sourceRoot": "apps/file-upload/src",
